        <TextBlock x:Name="textBlock2" HorizontalAlignment="Left" Margin="23,85,0,0" TextWrapping="Wrap" Text="Date : " VerticalAlignment="Top" Height="23" Width="49"/>
        <TextBox x:Name="textBox_date" HorizontalAlignment="Left" Height="23" Margin="77,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="438" IsEnabled="False"/>
        <GroupBox x:Name="groupBox" Header="Header" HorizontalAlignment="Left" VerticalAlignment="Top" Height="111" Width="522" Margin="10,10,0,0"/>
        <TextBox x:Name="textBox_mail" HorizontalAlignment="Left" Height="129" Margin="10,126,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="522" IsEnabled="False"/>

    </Grid>
</Window>
